<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.doancoso3.ui.fragment.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeRestaurantFragment"
            app:destination="@id/homeRestaurantFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.doancoso3.ui.fragment.SignUpFragment"
        android:label="SignUpFragment"
        tools:layout="@layout/fragment_sign_up">

    <action
            android:id="@+id/action_signUpFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.doancoso3.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_restaurantDetailFragment"
            app:destination="@id/restaurantDetailFragment">
        </action>
        <action
            android:id="@+id/action_homeFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment" />
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_homeFragment_to_userInfoFragment"
            app:destination="@id/userInfoFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myOrderFragment"
            app:destination="@id/myOrderFragment" />
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>

    <fragment
        android:id="@+id/restaurantDetailFragment"
        android:name="com.example.doancoso3.ui.fragment.RestaurantDetailFragment"
        android:label="RestaurantDetailFragment"
        tools:layout="@layout/fragment_restaurant_detail">
        <argument
            android:name="restaurantId"
            app:argType="integer"/>
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_restaurantDetailFragment_to_cartFoodFragment"
            app:destination="@id/cartFoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/cartFoodFragment"
        android:name="com.example.doancoso3.ui.fragment.CartFoodFragment"
        android:label="CartFoodFragment"
        tools:layout="@layout/fragment_cart_food">
        <argument
            android:name="restaurantId"
            app:argType="integer"/>
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_cartFoodFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.example.doancoso3.ui.fragment.FavoriteFragment"
        android:label="FavoriteFragment"
        tools:layout="@layout/fragment_favorite">
        <action
            android:id="@+id/action_favoriteFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_favoriteFragment_to_restaurantDetailFragment"
            app:destination="@id/restaurantDetailFragment" />
        <action
            android:id="@+id/action_favoriteFragment_to_userInfoFragment"
            app:destination="@id/userInfoFragment" />
        <action
            android:id="@+id/action_favoriteFragment_to_myOrderFragment"
            app:destination="@id/myOrderFragment" />
        <action
            android:id="@+id/action_favoriteFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeRestaurantFragment"
        android:name="com.example.doancoso3.ui.fragment.HomeRestaurantFragment"
        android:label="fragment_home_restaurant"
        tools:layout="@layout/fragment_home_restaurant">
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_homeRestaurantFragment_to_manageOrderFragment"
            app:destination="@id/manageOrderFragment">
            <argument
                android:name="username"
                app:argType="string"/>
        </action>
        <action
            android:id="@+id/action_homeRestaurantFragment_to_manageFoodFragment"
            app:destination="@id/manageFoodFragment">
            <argument
                android:name="username"
                app:argType="string"/>
        </action>
        <action
            android:id="@+id/action_homeRestaurantFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_homeRestaurantFragment_to_revenueFragment"
            app:destination="@id/revenueFragment">
            <argument
                android:name="username"
                app:argType="string"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/manageOrderFragment"
        android:name="com.example.doancoso3.ui.fragment.ManageOrderFragment"
        android:label="fragment_manage_order"
        tools:layout="@layout/fragment_manage_order">

        <action
            android:id="@+id/action_manageOrderFragment_to_orderDetailFragment"
            app:destination="@id/orderDetailFragment">
            <argument
                android:name="orderID"
                app:argType="integer"
                android:defaultValue="0" />
            <argument
                android:name="username"
                app:argType="string"/>
            <argument
                android:name="restaurantId"
                app:argType="integer"
                android:defaultValue="0" />
        </action>
        <action
            android:id="@+id/action_manageOrderFragment_to_homeRestaurantFragment"
            app:destination="@id/homeRestaurantFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailFragment"
        android:name="com.example.doancoso3.ui.fragment.OrderDetailFragment"
        android:label="fragment_order_detail"
        tools:layout="@layout/fragment_order_detail" >
        <action
            android:id="@+id/action_orderDetailFragment_to_manageOrderFragment"
            app:destination="@id/manageOrderFragment" />
    </fragment>
    <fragment
        android:id="@+id/userInfoFragment"
        android:name="com.example.doancoso3.ui.fragment.UserInfoFragment"
        android:label="fragment_user_info"
        tools:layout="@layout/fragment_user_info">
        <action
            android:id="@+id/action_userInfoFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_userInfoFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment" />
        <action
            android:id="@+id/action_userInfoFragment_to_myOrderFragment"
            app:destination="@id/myOrderFragment" />
    </fragment>
    <fragment
        android:id="@+id/myOrderFragment"
        android:name="com.example.doancoso3.ui.fragment.MyOrderFragment"
        android:label="fragment_my_order"
        tools:layout="@layout/fragment_my_order">
        <argument
            android:name="username"
            app:argType="string"/>
        <action
            android:id="@+id/action_myOrderFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_myOrderFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment" />
        <action
            android:id="@+id/action_myOrderFragment_to_userInfoFragment"
            app:destination="@id/userInfoFragment" />
        <action
            android:id="@+id/action_myOrderFragment_to_orderDetailUserFragment"
            app:destination="@id/orderDetailUserFragment" >
            <argument
                android:name="orderID"
                app:argType="integer"
                android:defaultValue="0" />
            <argument
                android:name="restaurantId"
                app:argType="integer"
                android:defaultValue="0" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/orderDetailUserFragment"
        android:name="com.example.doancoso3.ui.fragment.OrderDetailUserFragment"
        android:label="OrderDetailUserFragment"
        tools:layout="@layout/fragment_order_detail_user">
    </fragment>
    <fragment
        android:id="@+id/manageFoodFragment"
        android:name="com.example.doancoso3.ui.fragment.ManageFoodFragment"
        android:label="ManageFoodFragment"
        tools:layout="@layout/fragment_manage_food">
        <action
            android:id="@+id/action_manageFoodFragment_to_homeRestaurantFragment"
            app:destination="@id/homeRestaurantFragment" />
        <action
            android:id="@+id/action_manageFoodFragment_to_editFoodFragment"
            app:destination="@id/editFoodFragment">
            <argument
                android:name="food_id"
                app:argType="integer"
                android:defaultValue="0" />
            <argument
                android:name="restaurant_id"
                app:argType="integer"
                android:defaultValue="0" />
        </action>
        <action
            android:id="@+id/action_manageFoodFragment_to_addFoodFragment"
            app:destination="@id/addFoodFragment">
            <argument
                android:name="username"
                app:argType="string"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/editFoodFragment"
        android:name="com.example.doancoso3.ui.fragment.EditFoodFragment"
        android:label="EditFoodFragment"
        tools:layout="@layout/fragment_edit_food">
    </fragment>
    <fragment
        android:id="@+id/addFoodFragment"
        android:name="com.example.doancoso3.ui.fragment.AddFoodFragment"
        android:label="fragment_add_food"
        tools:layout="@layout/fragment_add_food" />
    <fragment
        android:id="@+id/revenueFragment"
        android:name="com.example.doancoso3.ui.fragment.RevenueFragment"
        android:label="RevenueFragment"
        tools:layout="@layout/fragment_revenue" >
        <action
            android:id="@+id/action_revenueFragment_to_homeRestaurantFragment"
            app:destination="@id/homeRestaurantFragment" />
        <action
            android:id="@+id/action_revenueFragment_to_orderDetailFragment"
            app:destination="@id/orderDetailFragment" >
            <argument
                android:name="orderID"
                app:argType="integer"
                android:defaultValue="0" />
            <argument
                android:name="username"
                app:argType="string"/>
            <argument
                android:name="restaurantId"
                app:argType="integer"
                android:defaultValue="0" />
        </action>
    </fragment>
</navigation>